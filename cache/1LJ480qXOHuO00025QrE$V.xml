			<Name>SpilloverLevel</Name>
			<PropertyType>
			</PropertyType>
			<Definition>null</Definition>
			<NameAliases>
				<NameAlias lang="ja-JP">水位</NameAlias>
			</NameAliases>
			<DefinitionAliases>
